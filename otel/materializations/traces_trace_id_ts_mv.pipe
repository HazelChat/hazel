DESCRIPTION >
    Materialized view to aggregate trace start and end times by TraceId

NODE traces_trace_id_ts_mv_node
SQL >
    SELECT
        TraceId,
        min(Timestamp) AS Start,
        max(Timestamp) AS End
    FROM traces
    WHERE TraceId != ''
    GROUP BY TraceId

TYPE MATERIALIZED
DATASOURCE traces_trace_id_ts