# OpenRouter API Key (required for AI streaming)
# Get yours at: https://openrouter.ai/keys
OPENROUTER_API_KEY=sk-or-v1-your-key-here

# Durable Streams server URL
STREAMS_SERVER_URL=http://localhost:8081

# Backend API URL (for persisting completed messages)
API_BASE_URL=http://localhost:3003

# Rivet actor server port
PORT=8082

# Rivet Engine endpoint (self-hosted Docker container)
RIVET_ENDPOINT=http://localhost:6420
